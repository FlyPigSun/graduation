<?php

/**
* hobby_tb model
*/

class Hobby_Student_Model  extends  CI_Model{

 var $id='';
 var $studentid='';
 var $hobbyid='';

public function insert($studentid,$hobbyid){
 $this->load->database();
 $sql="insert into hobby_tb value(null,?,?) ";
 $query=$this->db->query($sql,array($studentid,$hobbyid));
 $this->db->close();
}



}
?>